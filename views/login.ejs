<h1>Log in</h1>
<% if (messages.error) { %>
    <%= messages.error %>
  <% } %>
<form action="/login" method="POST">

    <div>
        <label for="email">Email</label>
        <input type="text" id="email" name="backendEmail" required>
        </div>

        <div>
            <label for="password">Password</label>
            <input type="password" id="password" name="backendPassword" required>
            </div>   
            
    
    <button type="submit" name="submit">Log in</button>  
</form>

<a href="/register">Registration</a>      
